#map
  width: 300vw
  height: 300vh
  position: fixed
  top: 0
  right: 0
  bottom: 0
  left: 0
  display: flex
  flex-wrap: wrap
  transform: translate(-33.3333333%,-33.333333%)

.map-block
  width: 100vw
  height: 100vh
  border: 1px solid red
  display: flex
  align-items: center
  justify-content: center
  font-size: 10em
